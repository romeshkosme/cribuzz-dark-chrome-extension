{
  "devDependencies": {
    "@parcel/config-webextension": "^2.12.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "parcel": "^2.12.0",
    "puppeteer": "^22.6.4"
  },
  "scripts": {
    "test": "jest ./tests__/",
    "start": "rm -rf dist .parcel-cache && parcel watch manifest.json 'scripts/*js' --host localhost --config @parcel/config-webextension",
